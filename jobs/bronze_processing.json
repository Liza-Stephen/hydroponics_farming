          "python_file": "/Repos/lizabinispam@gmail.com/hydroponics_farming/src/data_processing/bronze/batch_ingestion.py",
          "parameters": [
            "hydroponics",
            "/Volumes/hydroponics/bronze/raw_data/iot_data_raw.csv"
