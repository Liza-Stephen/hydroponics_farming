{
  "name": "ml_inference",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "inference",
      "run_if": "ALL_SUCCESS",
      "spark_python_task": {
        "python_file": "/Repos/lizabinispam@gmail.com/hydroponics_farming/src/ml/inference/batch_inference.py",
        "parameters": [
          "hydroponics",
          "hydroponics-data",
          "feature_store.sensor_features",
          "hydroponics.default.hydroponics_lstm_forecast",
          "hydroponics.default.hydroponics_gru_forecast",
          "hydroponics.default.hydroponics_lightgbm_classifier",
          "hydroponics.predictions.batch_predictions"
        ]
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {},
      "environment_key": "env"
    }
  ],
  "queue": {
    "enabled": true
  },
  "environments": [
    {
      "environment_key": "Default",
      "spec": {
        "environment_version": "4"
      }
    },
    {
      "environment_key": "env",
      "spec": {
        "dependencies": [
          "torch",
          "lightgbm>=4.0.0"
        ],
        "environment_version": "4"
      }
    }
  ],
  "performance_target": "PERFORMANCE_OPTIMIZED"
}