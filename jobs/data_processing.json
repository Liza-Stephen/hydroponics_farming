      "run_if": "ALL_SUCCESS",
      "spark_python_task": {
        "python_file": "/Repos/lizabinispam@gmail.com/hydroponics_farming/src/data_processing/gold/processing.py",
        "parameters": [
          "hydroponics",
          "hydroponics-data"
        ]
