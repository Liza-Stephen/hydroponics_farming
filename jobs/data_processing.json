{
  "name": "data_processing",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "schedule": {
    "quartz_cron_expression": "4 0 12 * * ?",
