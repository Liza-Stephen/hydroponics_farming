{
  "name": "data_processing",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "bronze_processing",
      "run_if": "ALL_SUCCESS",
      "spark_python_task": {
        "python_file": "/Repos/lizabinispam@gmail.com/hydroponics_farming/src/data_processing/bronze_ingestion.py"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {},
      "environment_key": "Default"
    },
    {
      "task_key": "silver_processing",
      "depends_on": [
        {
          "task_key": "bronze_processing"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "spark_python_task": {
        "python_file": "/Repos/lizabinispam@gmail.com/hydroponics_farming/src/data_processing/silver_processing.py"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {},
      "environment_key": "Default"
    },
    {
      "task_key": "gold_processing",
      "depends_on": [
        {
          "task_key": "silver_processing"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "spark_python_task": {
        "python_file": "/Repos/lizabinispam@gmail.com/hydroponics_farming/src/data_processing/gold_processing.py"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {},
      "environment_key": "Default"
    }
  ],
  "queue": {
    "enabled": true
  },
  "parameters": [
    {
      "name": "DATABRICKS_CATALOG",
      "default": "hydroponics"
    },
    {
      "name": "SOURCE_DATA_PATH",
      "default": "/Volumes/hydroponics/bronze/raw_data/iot_data_raw.csv"
    }
  ],
  "environments": [
    {
      "environment_key": "Default",
      "spec": {
        "environment_version": "4"
      }
    }
  ],
  "performance_target": "PERFORMANCE_OPTIMIZED"
}