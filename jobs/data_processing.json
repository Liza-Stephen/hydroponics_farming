        "python_file": "/Repos/lizabinispam@gmail.com/hydroponics_farming/src/data_processing/snowflake/processing.py",
        "parameters": [
          "hydroponics",
          "hydroponics-data",
          "GTNZFKD-YP25170",
          "databricks_user",
          "qwerty123",
          "HYDRO_WH",
